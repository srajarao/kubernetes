# PostgreSQL connection settings for FastAPI

# Database host (internal service name)
DB_HOST=postgres-db

# Database port (internal port)
DB_PORT=5432

# Database name
DB_NAME=postgres

# Database user
DB_USER=postgres

# Database password
DB_PASSWORD=postgres

# Optional: SQLAlchemy connection string
DATABASE_URL=postgresql://postgres:postgres@postgres-db:5432/postgres

# Transformers model for FastAPI
TRANSFORMERS_MODEL=distilbert-base-uncased

# Change 'your_database_name', 'your_username', and 'your_password' to your actual credentials.